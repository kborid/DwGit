<resources>
    <!-- XML attributes used by app_alias.xml -->
    <declare-styleable name="alias">
        <attr name="aliasName" format="string" />
        <attr name="realName" format="string" />
    </declare-styleable>
    <!-- XML attributes used by defalt_index.xml -->
    <declare-styleable name="favorites">
        <attr name="index" format="integer" />
        <attr name="lableName" format="string" />
        <attr name="icon" format="string" />
        <attr name="uri" format="string" />
    </declare-styleable>
    <declare-styleable name="RoundedRectListView">
        <attr name="radius" format="dimension" />
        <attr name="radius_marginTop" format="dimension" />
        <attr name="radius_marginLeft" format="dimension" />
        <attr name="radius_marginRight" format="dimension" />
        <attr name="radius_marginBottom" format="dimension" />
    </declare-styleable>
    <declare-styleable name="HorizontalScrollListView">
        <attr name="scroll_enabled" format="boolean" />
        <attr name="scroll_mode">
            <enum name="right" value="0" />
            <enum name="left" value="1" />
            <enum name="both" value="2" />
        </attr>
        <attr name="restore_eat_back_key" format="boolean" />
        <attr name="left_btns_width" format="dimension" />
        <attr name="right_btns_width" format="dimension" />
        <attr name="max_left_btns_width" format="dimension" />
        <attr name="max_right_btns_width" format="dimension" />
        <attr name="velocity_factor" format="float" />
        <attr name="overflow_velocity_factor" format="float" />
        <attr name="scroll_factor" format="float" />
        <attr name="scroll_handle_id" format="integer" />
        <attr name="left_btns_id" format="integer" />
        <attr name="right_btns_id" format="integer" />
    </declare-styleable>
    <declare-styleable name="SliderListView">
        <attr name="slider_checkbox_id" format="integer" />
        <attr name="slider_enabled" format="boolean" />
    </declare-styleable>
    <declare-styleable name="LetterIndexView">
        <attr name="letterWidth" format="dimension" />
        <attr name="letterHeight" format="dimension" />
        <attr name="letterSize" format="dimension" />
        <attr name="letterColor" format="color" />
        <attr name="letterArray" format="reference" />
        <attr name="letterMarginTop" format="dimension" />
        <attr name="letterMarginBottom" format="dimension" />
        <attr name="letterStyle">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
        </attr>
        <attr name="letterHeadArray" format="reference" />
        <attr name="letterHeadWidth" format="dimension" />
        <attr name="letterHeadHeight" format="dimension" />
        <attr name="letterHeadSpace" format="dimension" />
        <attr name="letterHeadMarginTop" format="dimension" />
        <attr name="letterHeadMarginBottom" format="dimension" />
        <attr name="letterNormalColor" format="color" />
        <attr name="letterTouchBackground" format="reference" />
    </declare-styleable>

    <declare-styleable name="ContactSelectView">
        <attr name="numColumns" format="integer" />
        <attr name="showLivIndex" format="boolean" />
        <attr name="multiple" format="boolean" />
        <attr name="showLabel" format="boolean" />
        <attr name="loadMore" format="boolean" />
        <attr name="sourceType">
            <enum name="friend" value="1" />
            <enum name="recentFriend" value="2" />
            <enum name="starFriend" value="3" />
            <enum name="starAndAllFriend" value="4" />
            <enum name="recentAndAllFriend" value="5" />
            <enum name="searchFriend" value="6"/>
        </attr>
        <attr name="searchType">
            <enum name="voice" value="1"/>
            <enum name="text" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>
</resources>