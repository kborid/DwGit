<resources>
    <declare-styleable name="LimitedLinearLayout">
        <attr name="orientation">
            <flag name="horizontal" value="0" />
            <flag name="vertical" value="1" />
        </attr>
        <attr name="order">
            <flag name="asc" value="0" />
            <flag name="desc" value="1" />
        </attr>
        <attr name="maxHeight" format="dimension" />
        <attr name="maxWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="OverlayEffectView">
        <attr name="overlayCount" format="integer" />
        <attr name="overlayWidth" format="dimension" />
        <attr name="borderPosition" format="integer" />
        <attr name="src" format="reference" />
    </declare-styleable>
</resources>
